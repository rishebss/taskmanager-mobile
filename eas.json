{
    "cli": {
      "version": ">= 7.7.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "APP_ENV": "development"
        }
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "env": {
          "APP_ENV": "staging"
        }
      },
      "production": {
        "autoIncrement": true,
        "android": {
          "buildType": "apk"
        },
        "env": {
          "APP_ENV": "production"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }